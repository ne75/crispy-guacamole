 * This program reads integers from a file and adds up the
 * number of integers > 500 and the number of integers < 500 
 * and prints these values along with the sum.
 * 
 */

import java.io.File;
import java.io.FileNotFoundException;
import java.util.*;

public class CountingNumbers {
    public static void main(String[] args) {
	
	File file = new File("p209a.dat");
	try {
	    Scanner s = new Scanner(file);
	} catch (FileNotFoundException e) {
	    e.printStackTrace();
	}
    }

}
