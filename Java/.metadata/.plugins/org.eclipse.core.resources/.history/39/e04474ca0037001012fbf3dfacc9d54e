public class deckOfCards {
	public static void main(String[] args) {
		
	int[] faceNumber;
	char[] suitNumber;
	int count = 0;
	int face = 0;
	char face_high = 0;
	char suit = 0;
	
	face = new int[13];
	suit = new char[4];
	
	for (count = 0; count < 13; count++){
		face = faceNumber[count];
		System.out.println(face);
	}
	}

}
